def isPalindrome(self, x: int) -> bool:
        if x<0:
            return False
        n = x
        rev_num = 0
        while(n>0):
            
            rev_num  = rev_num*10  + (n % 10)
           
            n = n//10
            
        if rev_num == x:
            return True
        else:
            return False
